{
  "expo": {
    "name": "react-native-template",
    "slug": "react-native-template",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darkviolet.divination-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/kufam/Kufam_400Regular_Italic.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_400Regular.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_500Medium_Italic.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_500Medium.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_600SemiBold_Italic.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_700Bold_Italic.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_700Bold.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_800ExtraBold_Italic.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_900Black_Italic.ttf",
            "node_modules/@expo-google-fonts/kufam/Kufam_900Black.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
